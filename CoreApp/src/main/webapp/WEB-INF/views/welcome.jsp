<body>
<div class="container">
    <h1>Student Details</h1>
    <table class="table table-hover">
        <thead>
            <tr>
                <th>Student ID</th>
                <th>Student Name</th>
                <th>Student Birthday</th>
                <th>Student gender</th>
                <th>Student Address</th>
            </tr>
        </thead>
        <tbody>
            <tr>
                <td>${student.id}</td>
                <td>${student.name}</td>
                <td>${student.birthday}</td>
                <td>${student.gendar[0]}</td>
                <td>${student.address}</td>
            </tr>
        </tbody>
    </table>
</div>

